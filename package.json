{
  "name": "Social-VR-Editor",
  "version": "0.0.2",
  "description": "Social VR Editor Application",
  "scripts": {
    "build:copy": "cp src/index.html dist/index.html; cp src/favicon.ico dist/favicon.ico; cp src/manifest.json dist/manifest.json; cp src/socialVr.png dist/socialVR.png; cp -r src/assets dist/assets; cp -r src/__stubs__ dist/__stubs__",
    "build:webpack": "webpack --colors --progress",
    "build:webpack-prod": "webpack --colors --progress -p",
    "build": "npm run clean && npm run build:webpack && npm run build:copy",
    "deploy": "npm run build && git-directory-deploy --directory dist --branch gh-pages",
    "build:prod": "npm run clean && npm run build:webpack-prod && npm run build:copy",
    "deploy:prod": "npm run build && git-directory-deploy --directory dist --branch gh-pages",
    "dev": "webpack-dev-server --inline --colors --progress --content-base src",
    "dev-prod": "webpack-dev-server --inline --colors --progress --content-base src -p",
    "dev-local": "webpack-dev-server --inline --colors --progress --content-base src -d",
    "clean": "rm -rf dist",
    "clean:node": "npm run clean; rm -rf node_modules",
    "lint": "tslint  --type-check --project tslint.json"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.32",
    "@types/node": "^6.0.38",
    "@types/three": "^0.89.12",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.0",
    "babel-core": "^6.24.1",
    "css-loader": "^0.23.1",
    "gh-pages": "^0.11.0",
    "git-directory-deploy": "^1.5.1",
    "html-webpack-plugin": "^2.30.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^2.0.5",
    "promise-polyfill": "^6.0.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "to-string-loader": "^1.1.4",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular/common": "^4.3.3",
    "@angular/compiler": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@angular/forms": "^4.3.3",
    "@angular/http": "^4.3.3",
    "@angular/platform-browser": "^4.3.3",
    "@angular/platform-browser-dynamic": "^4.3.3",
    "@angular/router": "^4.3.3",
    "@tweenjs/tween.js": "^17.1.1",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.3",
    "firebase": "^4.3.1",
    "ie-shim": "^0.1.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.7.0",
    "jszip": "3.1.4",
    "qrcode": "^1.2.0",
    "recorderjs": "git+ssh://git@github.com/mattdiamond/Recorderjs.git#08e7abd99739be6946f19f6806ccb368138f2dd3",
    "rxjs": "^5.4.2",
    "serviceworker-webpack-plugin": "^0.2.3",
    "stats.js": "^0.17.0",
    "three": "^0.91.0",
    "webvr-polyfill": "^0.10.4",
    "zone.js": "^0.8.16"
